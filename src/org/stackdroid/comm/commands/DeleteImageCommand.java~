package org.stackdroid.comm.commands;

import android.util.Log;

public class CommandListImage extends Command {
	public void execute( ) throws NotAuthorizedException, NotFoundException, ServerException, ServiceUnAvailableOrInternalError,
				           IOException, MalformedURLException, ProtocolException, ParseException,CertificateException
	{
		checkToken( );
		return RESTClient.sendGETRequest(U.useSSL(),
						      			 U.getGlanceEndpoint() + "/" + U.getGlanceEndpointAPIVER() + "/images",
						      			 U.getToken(),
						      			 null);
	}
}